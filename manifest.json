{
	"manifest_version": 3,
	"name": "Tabify",
	"description": "Checks site URLs of websites to prevent opening the same link twice. Closes duplicate tabs and selects the old tab instead.",
	"author": "William Martin",
	"version": "2.1.2",
	"icons": {
		"16": "/assets/images/blue_happy_16.png",
		"48": "/assets/images/blue_happy_48.png",
		"128": "/assets/images/blue_happy_128.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_title": "Tabify",
		"default_icon": {
			"16": "/assets/images/blue_happy_16.png",
			"48": "/assets/images/blue_happy_48.png",
			"128": "/assets/images/blue_happy_128.png"
		}
	},
	"permissions": ["storage", "tabs", "activeTab", "contextMenus"],
	"host_permissions": ["http://*/*", "https://*/*"],
	"background": {
		"service_worker": "background.js"
	}
}
